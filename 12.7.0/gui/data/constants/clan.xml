<?xml version="1.0"?>
<constants>
    <enum name="CLAN_ROLE" type="String">
        <const name="EXECUTIVE_OFFICER" value="executive_officer"/>
        <const name="PRIVATE" value="private"/>
        <const name="OFFICER" value="officer"/>
        <const name="COMMISSIONED_OFFICER" value="commissioned_officer"/>
        <const name="RECRUITMENT_OFFICER" value="recruitment_officer"/>
        <const name="COMMANDER" value="commander"/>
    </enum>

    <enum name="CLAN_BATTLE_STAGE" type="String">
        <const name="PROMOTION" value="promotion"/>
        <const name="DEMOTION" value="demotion"/>
        <const name="LEAGUE" value="league"/>
    </enum>

    <enum name="CLAN_BATTLE_PROGRESS" type="String">
        <const name="VICTORY" value="victory"/>
        <const name="DEFEAT" value="defeat"/>
        <const name="DRAW" value="draw"/>
        <const name="UNKNOWN" value="unknown"/>
    </enum>

