        <const name="HURRICANE" value="0xAB45E6"/> <!-- 0 -->
        <const name="TYPHOON" value="0xE5124A"/> <!-- 1 -->
        <const name="STORM" value="0xF1AE07"/> <!-- 2 -->
        <const name="GALE" value="0x0644D9"/> <!-- 3 -->
        <const name="SQUALL" value="0x00D8D0"/> <!-- 4 -->
        <const name="DEFAULT" value="0xFFFFFF"/>
    </enum>

    <enum name="CLAN_MESSAGE_OF_THE_DAY_STATE" type="Integer">
        <const name="DEFAULT"               value="0"/>
        <const name="PRE_MODERATION"        value="1"/>
        <const name="PRE_MODERATION_OTHER"  value="2"/>

        <value_to_name/>
